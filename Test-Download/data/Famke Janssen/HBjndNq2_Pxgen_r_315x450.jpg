                var d = new Date(expiration);
                var n = new Date();
                var segments = [experimentID + ',' + experimentName + ',A', ',,', experimentID + ',' + experimentName + ',B'];
                var randomNumber = d > n ? signum(Math.max(0, percentage - Math.random()) * signum(Math.random() - 0.5)) + 1 : 1;
