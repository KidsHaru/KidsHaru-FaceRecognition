                                teaserClass = "teaser teaser--two-cols";
                                teaserWidth = 420;
                            } else if (menuLevel === 2 || menuLevel === 1) {
                                teaserClass = "teaser teaser--three-cols";
                                teaserWidth = 644;
                            } else if (menuLevel === 3) {
                                teaserClass = "teaser teaser--two-cols";
                                teaserWidth = 420;
                            }

                            teaser.attr("class", teaserClass);
                            teaser.attr("menu-path", menuPath);
                            teaser.css("width", teaserWidth);
