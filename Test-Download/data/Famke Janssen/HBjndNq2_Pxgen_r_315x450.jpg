        	}
        	var url = url.replace(window.location.origin,"");
        	if(url === "/" || url === ""){
        		return "home";
        	}
        	url = url.substring(url.indexOf("//")+2,url.indexOf(".html") >=0 ?url.indexOf(".html"):url.length-1);
        	return url;
        }
        
