<script>
    function HomepageTestTopNavUtils () {
        var isIpad = window.navigator && window.navigator.platform === 'iPad'

        if (isIpad) {
            document.body.classList.add('is-ipad')
        }

        var handleClick = function () {
            event.preventDefault()
            var active = 'is-active';
            if (isIpad) {
                var openMenu = document.getElementById('js-header-nav').getElementsByClassName(active);

                if(openMenu.length) {
                    Array.prototype.forEach.call(openMenu, function(el){
                        if(el != event.currentTarget) {
                            el.classList.remove(active)
                        }
                    });
                }

                event.currentTarget.classList.toggle(active)

            }
        }

        var closeParent = function () {
            event.preventDefault()

            if (!isIpad) {
                return
            }

            var parent = event.currentTarget.parentNode;
            var listItem = parent.parentNode.parentNode;
            var linkItem = listItem.querySelector('.test-header__current-vehicle-type');
            linkItem.classList.remove('is-active')
        }

        return { handleClick: handleClick, closeParent: closeParent }
    }

    var homepageTestTopNavUtils = HomepageTestTopNavUtils()
</script>
<script>
    window.addEventListener('DOMContentLoaded', function() {

        var peekNav = document.querySelector('.js-peek-nav');

        if(peekNav !== null) {

            var scrollActive = false,
                previousScroll = 0;

            window.addEventListener('scroll', function() {
                scrollActive = true;
            }, false);

            setInterval(function() {
                if(scrollActive) {
                    var offSet = window.pageYOffset || document.documentElement.scrollTop;

                    if(offSet > previousScroll && (offSet-previousScroll) > 28 && offSet > 28) {
                        peekNav.classList.remove('is-active');
                        document.body.classList.remove('has-channel-nav');
                    } else if(offSet < previousScroll && (previousScroll-offSet) > 28 || offSet === 0) {
                        peekNav.classList.add('is-active');
                        document.body.classList.add('has-channel-nav')
                    }

                    previousScroll = offSet;
                    scrollActive = false;
                }

            }, 300);

        }
    }, false);
</script>
