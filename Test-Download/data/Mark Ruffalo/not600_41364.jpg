  function validarBusqueda(mobile){
      var campoSearch = "";
      if(mobile){
          campoSearch = $("#searchMobile").val();
      }else{
          campoSearch = $("#search").val();
      }
      var result = "";

      if (typeof campoSearch != 'undefined' && campoSearch.length > 0){
          var forbiddenchars = "%/\\&\"><";
            for(var i=0; i< campoSearch.length;i++){
                var aux = campoSearch.charAt(i);
                if( forbiddenchars.indexOf(aux)!= -1) {
                  result+= " ";
                }
                else{
                  result+= aux;
                }
            }
            window.location.href="https://atencionalcliente.movistar.es/?q=" + encodeURI(result) + "&vgnextoid=&action=resultadosBusqueda&controlBusqueda=avanzada&num_resultados=10&segmento=0&pid=AV-buscador&segmento=Particulares";
      }else{
        //alert(L["buscador.vacio"]);
        return false;
      }
  }

