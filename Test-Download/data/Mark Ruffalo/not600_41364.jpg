<script type="text/javascript">

var Cookie = {
/*devuelve el valor de la cookie al pasarle el nombre de la misma*/
getCookie: function(name){
  var cookies = document.cookie;
  var start = cookies.indexOf(name+"=");
  var len = start + name.length + 1;
  if ((!start) && (name != cookies.substring(0, name.length))){
    return null;
  }
  if (start == -1) return null;
  var end = cookies.indexOf(';', len);
  if (end == -1) end = cookies.length;
  return unescape(cookies.substring(len, end));
},
setCookie: function(name, value, expires, path, domain, secure){
  var today = new Date();
  today.setTime(today.getTime());
  if (expires!=null){
    expires = expires * 1000 * 60 * 60 * 24;
  }
  var expires_date = new Date(today.getTime() + (expires));
  document.cookie = name+'='+escape( value ) +
    ((expires!=null)? ';expires='+expires_date.toGMTString() : '' ) + //expires.toGMTString()
    ((path!=null)? ';path=' + path : '' ) +
    ((domain!=null)? ';domain=' + domain : '' ) +
    ((secure!=null)? ';secure' : '' );
},
deleteCookie: function(name, path, domain){
  var valCookie = this.getCookie(name);
  if (valCookie!=null){
    document.cookie = name + '=' +
    ((path!=null)?';path=' + path : '') +
    ((domain!=null)?';domain=' + domain : '' ) +';expires=Thu, 01-Jan-1970 00:00:01 GMT';
  }
},
ajaxDeleteCookieSession:function(_url){
  jQuery.ajax({
      url:_url,
      cache: false,
      type: "POST",
      timeout:1000000000,
    data: "",
    success:function(datos){
      if(document.getElementById('loginzone')!=null){
        // remove all children from element
        document.getElementById('loginzone').innerHTML = '';
      }
      },
    error:function(jqXHR, textStatus, errorThrown){
      if(textStatus==="timeout") {
        document.write("<!--zzz Error timeout en ajaxDeleteCookie errorThrown: ["+ errorThrown +"], textStatus ["+textStatus+"] -->");
          }else{
            document.write("<!--zzz Error textStatus ajaxDeleteCookie ["+textStatus+"] -->");
          }
    }
  });
}
}

