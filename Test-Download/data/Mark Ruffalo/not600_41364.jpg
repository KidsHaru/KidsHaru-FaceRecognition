$(document).ready(function(){});

/*condicion inical*/
$('.itemBloqueMenu .listaLinks').attr('desplegado','0');
/*logica*/
$('.itemBloqueMenu .lineaMenu').click(function (){
var estamosEnMovil=$('#estamosEnMovil').css('opacity');
console.log('estamosEnMovil: '+estamosEnMovil)
if(estamosEnMovil=='1'){
  /*TOMO AL PADRE*/
  var elementoDesplegable=$(this).parent().find('.listaLinks');
  var dataid=elementoDesplegable.attr('data-id');
  var desplegado=elementoDesplegable.attr('desplegado');
  if(desplegado=="0"){
    //console.log('despleigo');
    elementoDesplegable.slideDown(function(){ elementoDesplegable.attr('desplegado','1'); });
  }else{
    //console.log('pliego');
    elementoDesplegable.slideUp(function(){ elementoDesplegable.attr('desplegado','0'); });
  }
  pliegaTodos(dataid);
}
});
function pliegaTodos(exp){
//console.log('exception: '+exp);
$('.itemBloqueMenu .listaLinks').each(function(){
  var dataid=$(this).attr('data-id');
  if(dataid!=exp){
    $(this).attr('desplegado','0');
    $(this).slideUp();
  }else{console.log('proceso la excepcion');}
});
}
/*caso excepcional uso movil y vuelta a pc, realmente.. se puede obviar es muy raro que un usuario pase de móvil a pc*/
