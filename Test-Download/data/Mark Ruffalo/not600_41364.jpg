         #encuestaClasico ul li input[type=radio] {
         position: absolute;
         visibility: hidden;
         }
         #encuestaClasico ul li label {
         display: block;
         position: relative;
         font-weight: 300;
         font-size: 1.35em;
         padding: 0px 25px 0px 80px;
         margin: 0px auto;
         z-index: 9;
         cursor: pointer;
         -webkit-transition: all 0.25s linear;
         }
         #encuestaClasico ul li:hover label {
         color: #FFFFFF;
         }
         #encuestaClasico ul li .check {
         display: block;
         position: absolute;
         border: 2px solid #ffffff;
         border-radius: 100%;
         height: 25px;
         width: 25px;
         top: 0px;
         left: 20px;
         z-index: 5;
         transition: border .25s linear;
         -webkit-transition: border .25s linear;
         margin: 7px auto;
         }
         #encuestaClasico ul li:hover .check {
         border: 2px solid #FFFFFF;
         }
         #encuestaClasico ul li .check::before {
         display: block;
         position: absolute;
         content: '';
         border-radius: 100%;
         height: 15px;
         width: 15px;
         top: 3px;
         left: 3px;
         margin: auto;
         transition: background 0.25s linear;
         -webkit-transition: background 0.25s linear;
         }
         #encuestaClasico input[type=radio]:checked~.check {
         border: 2px solid #5bc500;
         }
         #encuestaClasico input[type=radio]:checked~.check::before {
         background: #5bc500;
         }
         #encuestaClasico input[type=radio]:checked~label {
         color: #5bc500;
         }
         @media (max-width: 765px) {
         #encuestaClasico {
         display:none !important;
