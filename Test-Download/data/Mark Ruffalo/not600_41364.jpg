        $('#GW_menuCabecera').removeClass("in");
        
    });
      
    $('#GW_menuCabecera .dropdown-submenu a.dropdown-submenu-items').on("click", function(e){
      var displayed = $(this).next('ul').css("display");
      $('.dropdown-submenu-items-ul').hide();
      if(displayed == 'none'){
                  $(this).next('ul').show();
      }

      e.stopPropagation();
      e.preventDefault();
    });
      
      
    //Implementación de las métricas para el menú de la home
    
    //Menu Desktop (tercer nivel)
    $('.col-sm-2.col-xs-12 .menu-nivel2 a').click(function(e){
        
        var title = $(this).attr("title");

        title = normalizeStrForAnalytics(title);

        var menuLevel1 = $(this).parent().parent().children('.row').text();
        menuLevel1 = normalizeStrForAnalytics(menuLevel1);

        if(menuLevel1 == ""){
            menuLevel1 = "mas contenidos"
        }

        var mainMenu = $(this).parents('ul').parent().children('.menu-nivel1').text();
        mainMenu = normalizeStrForAnalytics(mainMenu);


        dataLayerGTM.push({
            'GAcategory':'menu principal',
            'GAaction':'clic - ' + mainMenu + ':' + menuLevel1 + ':' + title,
            'GAlabel':s.pageName,
            'event':'asyncGTM'
        });
    });

    //Menú primer nivel 
    $('.menu-nivel1[data-toggle="dropdown"]').click(function(e){
        var texto = normalizeStrForAnalytics($(this).text());
        
        dataLayerGTM.push({
            'GAcategory':'menu principal',
            'GAaction':'clic - ' + texto,
            'GAlabel':s.pageName,
            'event':'asyncGTM'
        });
    });

    //Menú mobile (tercer nivel)
    $('.link-nivel2-styles').click(function(e){

        var title = $(this).attr("title");
        title = normalizeStrForAnalytics(title);

        var menuLevel1 = $(this).closest('ul').parent().find('.title-menu.dropdown-nivel2-estilos').text();
        menuLevel1 = normalizeStrForAnalytics(menuLevel1);

        if(menuLevel1 == ""){
            menuLevel1 = "mas contenidos"
        }

        var mainMenu = $(this).parents('.position-initial').children('.menu-nivel1').text();
        mainMenu = normalizeStrForAnalytics(mainMenu);

        dataLayerGTM.push({
            'GAcategory':'menu principal',
            'GAaction':'clic - ' + mainMenu + ':' + menuLevel1 + ':' + title,
            'GAlabel':s.pageName,
            'event':'asyncGTM'
        });
    });
      
    
    //Evento logo del menu desktop
    $('.logo-movistar').click(function(e)
    {
        dataLayerGTM.push({
            'GAcategory':'menu principal',
            'GAaction':'clic – inicio',
            'GAlabel':s.pageName,
            'event':'asyncGTM'
        }); 
    });
      
  });

