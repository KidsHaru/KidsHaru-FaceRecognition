                        method: "GET",
                        propertyToSearch: "Name",
                        tokenValue: "TokenValue",
                        theme: "facebook",
                        preventDuplicates: true
                    });
                    if ($(this).attr("data-tags-value") != '') {
                        var spl = $(this).attr("data-tags-value").split(',');
                        for (var s in spl) {
                            var spl2 = spl[s].split('%%');
                            $(this).tokenInput("add", { TokenValue: spl[s], Name: spl2[0] })
                        }
                    }
                });                
            });
