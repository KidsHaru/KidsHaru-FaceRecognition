	margin:18px 0 0 0;
}
.footer-informacyjne-list:last-child {
	border-right: 0px solid #e3e3e3;
	padding:0 14px 0 14px;
}
.footer-box-center{
	width:300px;
	margin-right: auto;
 	margin-left: auto;
}
#footer-informacyjne{
	background: rgb(247,247,247); /* Old browsers */
